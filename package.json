{
  "name": "plicely",
  "version": "1.0.0",
  "description": "Placetime marks you can share.",
  "main": "src/index.js",
  "scripts": {
    "dist": "http-server dist",
    "dev": "http-server -S -C cert.pem -o dist",
    "start": "webpack-dev-server --open --host 0.0.0.0",
    "test": "echo \"Error: no test specified\" && exit 1",
    "gencert": "openssl req -newkey rsa:2048 -new -nodes -x509 -days 3650 -keyout key.pem -out cert.pem",
    "build": "webpack --mode=production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codealchemist/plicely.git"
  },
  "keywords": [
    "geolocation",
    "web",
    "share",
    "place",
    "time",
    "placetime"
  ],
  "author": "Alberto Miranda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codealchemist/plicely/issues"
  },
  "homepage": "https://github.com/codealchemist/plicely#readme",
  "dependencies": {
    "http-server": "^0.11.1",
    "humanize-duration": "^3.16.0"
  },
  "devDependencies": {
    "standard-focus": "^1.1.2",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  }
}
